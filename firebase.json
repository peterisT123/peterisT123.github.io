{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "redirects": [
      {
        "source": "/:path*",
        "destination": "https://intasaptauja.app/:path*",
        "type": 301
      }
    ]
  }
}